@article{inoue_oscillation_2004,
	title = {Oscillation phenomenon of laser-sustained plasma in a {CW} laser propulsion},
	volume = {73},
	issn = {0042-207X},
	url = {https://www.sciencedirect.com/science/article/pii/S0042207X03002926},
	doi = {10.1016/j.vacuum.2003.12.063},
	series = {The 4th International Symposium on Applied Plasma Science},
	abstract = {Oscillation phenomenon of the laser-sustained plasma ({LSP}) has been observed through our experiments. The oscillation was caused by the fluctuation of the laser power. Such fluctuation would be inevitable due to nonuniformity of the atmosphere conditions and other reasons. In this study fundamental study on the {LSP} oscillation was carried out since it is expected that the thruster performance will be improved by the suppression of the oscillation. A model of the plasma oscillation was proposed, and its validity was evaluated from comparison with the experimental results. According to the model, it was shown that the oscillation would be suppressed by using the lens of small focusing length. The experimental results support this idea.},
	pages = {433--438},
	number = {3},
	journaltitle = {Vacuum},
	shortjournal = {Vacuum},
	author = {Inoue, Takayoshi and Ijiri, Tatsuya and Hosoda, Satoshi and Kojima, Kohei and Uehara, Susumu and Komurasaki, Kimiya and Arakawa, Yoshihiro},
	date = {2004-04-19},
	langid = {english},
	keywords = {{CW} laser propulsion, Laser supported combustion wave, Laser-sustained plasma, Plasma oscillation, Plasma stability},
	file = {ScienceDirect Snapshot:C\:\\Users\\Emmanuel\\Zotero\\storage\\ENTLVNJX\\S0042207X03002926.html:text/html},

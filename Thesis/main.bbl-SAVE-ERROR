    \entry{khanGasDetectionUsing2019}{article}{}{}
      \name{author}{3}{}{%
        {{hash=03aa1bff3174194d51e8a7fad7d5cb8a}{%
           family={Khan},
           familyi={K\bibinitperiod},
           given={Sulaiman},

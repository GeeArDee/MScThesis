@report{krier_energy_1988,
	title = {Energy Conversion Measurements in Laser-Sustained Argon Plasmas for Application to Rocket Propulsion.},
	url = {https://apps.dtic.mil/sti/citations/ADA194931},
	abstract = {Laser Propulsion is the production of high specific impulse rocket thrust using a high power laser as a remote energy source. Specific impulses in excess of 1000 seconds are achievable because propellant temperatures are very high and low molecular weight gases can be used. This report focuses on the energy conversion mechanisms of laser-sustained plasmas in flowing argon. The status of {AFOSR} sponsored experiments to determine thermal efficiency and global absorption is detailed. An improved testing facility has allowed plasma operating conditions never before possible. The results indicate that nearly all of the input laser power can be absorbed by a plasma. Plasmas at elevated gas pressure have been tested, and preliminary results presented. Optimal operating conditions have yet to be determined for the available laser powers and gas pressures. Further experimentation at very high argon gas velocities  20 ms must be performed in order to completely characterize plasma behavior. Keywords Beamed energy propulsion, Laser plasma formation.},
	pages = {85},
	number = {{UILU}-{ENG}-88-4006},
	institution = {Department of Mechanical and Industrial Engineering University of Illinois at Urbana-Champaign},
	type = {Technical Report},
	author = {Krier, Herman and Mazumder, Jyoti and Zerkle, David K. and Mertogul, Ayhan and Schwartz, Scott},
	urldate = {2022-08-05},
	date = {1988-04-15},
	langid = {english},
	note = {Section: Technical Reports},
}

      \strng{namehash}{c48a865568ebad7d7ae02798deef5679}
      \strng{fullhash}{c48a865568ebad7d7ae02798deef5679}
      \strng{fullhashraw}{c48a865568ebad7d7ae02798deef5679}
      \strng{bibnamehash}{c48a865568ebad7d7ae02798deef5679}
      \strng{authorbibnamehash}{c48a865568ebad7d7ae02798deef5679}
      \strng{authornamehash}{c48a865568ebad7d7ae02798deef5679}

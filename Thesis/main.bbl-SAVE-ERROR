      \strng{namehash}{2b3920c549be51e2694a8d0937b69a07}
      \strng{fullhash}{ddb72a801c7dc7ee5db063d73fef8cdc}
      \strng{fullhashraw}{ddb72a801c7dc7ee5db063d73fef8cdc}
      \strng{bibnamehash}{ddb72a801c7dc7ee5db063d73fef8cdc}
      \strng{authorbibnamehash}{ddb72a801c7dc7ee5db063d73fef8cdc}
      \strng{authornamehash}{2b3920c549be51e2694a8d0937b69a07}
